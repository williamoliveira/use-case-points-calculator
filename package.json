{
  "name": "laravel-debugger",
  "version": "0.0.1",
  "description": "laravel-debugger node server",
  "scripts": {
    "serve": "webpack-dev-server --config webpack.config.dev.js --watch --progress --colors",
    "serve-livereload": "webpack-dev-server --config webpack.config.dev.js --watch --progress --colors --inline",
    "watch-dev": "webpack --config webpack.config.dev.js --progress --colors --watch",
    "build-dev": "webpack --config webpack.config.dev.js --progress --colors",
    "build-prod": "webpack --config webpack.config.production.js --progress --colors"
  },
  "dependencies": {
    "angular": "^1.5",
    "angular-formly": "^8.2.1",
    "angular-formly-templates-bootstrap": "^6.4.0",
    "angular-ui-bootstrap": "^1.3.2",
    "angular-ui-router": "^0.3.0",
    "api-check": "^7.5.5",
    "bootstrap": "^3.3",
    "font-awesome": "^4.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "bower": "^1.7.9",
    "css-loader": "~0",
    "exports-loader": "~0",
    "extract-text-webpack-plugin": "~1",
    "file-loader": "~0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "~2",
    "ng-annotate-loader": "~0",
    "ngtemplate-loader": "~1",
    "style-loader": "~0",
    "url-loader": "~0",
    "webpack": "~1.13",
    "webpack-dev-server": "^1.14.1"
  }
}
